<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constDesing"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:ignore="InvalidId,MissingDefaultResource">

    <include
        android:id="@+id/circle_top"
        layout="@layout/circle_top_widget"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="@string/dev_nick"
        android:textColor="@android:color/darker_gray"
        android:textSize="12dp" />
    <include
        android:id="@+id/circle_botton"
        layout="@layout/circle_botton_widget"/>

</RelativeLayout>